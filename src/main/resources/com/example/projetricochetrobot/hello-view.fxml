<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.GridPane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.projetricochetrobot.HelloController">
    <children>
        <HBox>
            <children>
                <VBox>
                    <children>
                        <Button mnemonicParsing="false" onAction="#visionJoueur" prefWidth="70.0" text="Jouer">
                            <VBox.margin>
                                <Insets bottom="5.0" right="5.0" top="5.0" />
                            </VBox.margin>
                        </Button>
                    </children>
                </VBox>
                <GridPane fx:id="plateauDeJeu" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="640.0" prefWidth="640.0">
                </GridPane>
            </children>
        </HBox>
        <Label fx:id="statusLabel" alignment="CENTER" maxWidth="2" text="Label"/>
    </children>
</VBox>
